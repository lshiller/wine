						{
							"name": "profile",
							"components": [
								{
									"name": "edit_form",
									"type": "form",
									"mode": "update",
									"title": "Edit your profile",
									"submit_route": "user_settings.profile",
									"query_name": "current_user_data",
									"fields": [
										{
											"name": "profile.name",
											"title": "Name",
											"required": true,
											"type": "string"
										},
										{
											"name": "profile.email",
											"title": "E-mail",
											"type": "email",
											"required": true
										},
										{
											"name": "profile.facebook",
											"title": "Facebook URL",
											"type": "string"
										},
										{
											"name": "profile.google",
											"title": "Google+ URL",
											"type": "string"
										},
										{
											"name": "profile.twitter",
											"title": "Twitter ID",
											"type": "string"
										},
										{
											"name": "profile.website",
											"title": "Website URL",
											"type": "string"
										}
									]
								}
							],
							"type": "page"
						},
						{
							"name": "change_pass",
							"template": "change_pass",
							"type": "page"
						}
					],
					"components": [
						{
							"name": "side_menu",
							"type": "menu",
							"class": "nav nav-stacked nav-pills",
							"items": [
								{
									"title": "Profile",
									"route": "user_settings.profile"
								},
								{
									"title": "Change password",
									"route": "user_settings.change_pass"
								}
							]
						}
					],
					"type": "page"
				},
				{
					"name": "logout",
